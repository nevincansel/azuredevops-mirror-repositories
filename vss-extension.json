{
    "manifestVersion": 1,
    "id": "mirror-repos",
    "name": "Mirror Repositories",
    "version": "1.1.7",
    "publisher": "NevinCansel",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "tags": [
        "git",
        "clone",
        "push",
        "mirror",
        "repo",
        "repository"
    ],
    "description": "Mirroring two repositories.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "mirrorRepos"
        }
    ],
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/nevincansel/azure-devops-mirror-repositories"
      },
    "contributions": [
        {
            "id": "mirrorRepos",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "mirrorRepos"
            }
        }
    ]
}